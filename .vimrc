set nocompatible

filetype on

filetype plugin on

filetype indent on

inoremap jk <Esc>

syntax on

set bg=dark

set number

set tabstop=2

set shiftwidth=4

set cursorline

set wildmenu

set wildmode=list:longest

set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx

set showmatch

set hlsearch

set showcmd

set showmode

set smartcase

set ignorecase

set backspace=indent,eol,start
